[
  {
    "corsRuleName": "allow-boitex-web",
    "allowedOrigins": [
      "https://app.boitexinfo.com",
      "http://localhost:5000"
    ],
    "allowedHeaders": [
      "authorization",
      "content-type",
      "x-bz-file-name",
      "x-bz-content-sha1",
      "x-bz-part-number",
      "x-bz-info-author"
    ],
    "allowedOperations": [
      "b2_upload_file",
      "b2_upload_part",
      "b2_download_file_by_name",
      "b2_download_file_by_id"
    ],
    "exposeHeaders": ["x-bz-content-sha1"],
    "maxAgeSeconds": 3600
  }
]
